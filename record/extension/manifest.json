{
    "name": "Record",
    "description": "Record Extension",
    "version": "1.0",
    "icons": {
        "128": "icon.png"
    },
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "key": "",
    "permissions": [
        "activeTab",
        "tabs",
        "tabCapture",
        "storage",
        "downloads"
    ]
}